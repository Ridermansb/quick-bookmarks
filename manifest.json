{
    "name": "__MSG_extensionName__",
    "version": "0.1.3",
    "description": "Bookmarks on new tab",
    "default_locale": "pt_BR",
    "omnibox": {
        "keyword": "find"
    },
    "icons": {
        "16": "16.png",
        "24": "24.png",
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs",
        "i18n",
        "management",
        "windows",
        "bookmarks",
        "notifications",
        "unlimitedStorage"
    ]
}